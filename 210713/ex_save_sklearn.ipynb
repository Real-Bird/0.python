   "source": [
    "from sklearn.linear_model import LinearRegression\n",
    "import joblib\n",
    "\n",
    "x = [[10],[5],[9],[7]] # 공부시간\n",
    "y = [[100],[50],[90],[77]] #시험점수\n",
    "\n",
    "model = LinearRegression()\n",
    "model = model.fit(x,y)\n",
    "result = model.predict([[7]])\n",
    "print(result)\n",
    "\n",
    "#파일에 학습결과 저장\n",
    "joblib.dump(model, filename=\"score.pkl\") #pikle"
   ]
